import { Button, VerticalBox } from "std-widgets.slint";

export component StartScreen inherits Rectangle {
    callback on-confirm();
    
    VerticalBox {    
        Text {
            text: "Start Screen";
        }    
        
        Button {
            text: "Confirm";
            
            clicked => {
                root.on-confirm();
            }
        }
    }
}